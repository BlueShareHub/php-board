# 데이터베이스 호스트 설정
DB_HOST=localhost  # 예: localhost, 또는 DB 서버의 IP 주소

# 데이터베이스 이름 설정
DB_NAME=php_board  # 예: 사용할 데이터베이스 이름

# 데이터베이스 사용자명 설정
DB_USER=your_db_username  # 예: 데이터베이스 사용자 이름

# 데이터베이스 비밀번호 설정
DB_PASS=your_db_password  # 예: 데이터베이스 비밀번호

# 데이터베이스 포트 설정 (기본값: 3306)
DB_PORT=3306  # 예: 데이터베이스 서버의 포트 번호

# 데이터베이스 문자 집합 설정
DB_CHARSET=utf8mb4  # 예: utf8mb4

# 관리자 비밀번호 설정
ADMIN_PASSWORD=your_admin_password  # 예: 관리자 비밀번호
